<div class="container">
  <%=link_to"Add User",new_user_path,class: "btn btn-primary"%>
  <%= link_to"Go to Article Dashboard",articles_path,class: "btn btn-info" %>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Phone</th>
        <th scope="col">Show</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
       <%@users.each do |f|%>
      <tr>
        <th scope="row"><%=f.id%></th>
        <td><%=f.name%></td>
        <td><%=f.phone%></td>
        <td><%=f.email%></td>
        <td><%=link_to"Show",user_path(f.id),class: " btn btn-primary"%></td>
        <td><%= link_to "Destroy", user_path(f), data: {
                      method: :delete,
                      confirm: "Are you sure?"
                    }, class: "btn btn-danger"%>
        </td>
      </tr>
    </tbody>
    <% end %>
  </table>  
<div>
